{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true,
        "node":true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "comma-dangle": [2, "never"],
        "block-scoped-var": 0,
        "no-alert": 0,
        "no-array-constructor": 2,
        "no-caller": 1,
        "no-class-assign": 2,
        "no-cond-assign": 2,
        "no-const-assign": 2,
        "no-div-regex": 1,
        "no-dupe-keys": 2,
        "no-dupe-args": 2,
        "no-duplicate-case": 2,
        "no-else-return": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-ex-assign": 2,
        "no-extra-bind": 2,
        "no-extra-parens": 2,
        "no-extra-semi": 2,
        "no-floating-decimal": 2,
        "no-func-assign": 2,
        "no-implicit-coercion": 1,
        "no-invalid-regexp": 2,
        "no-invalid-this": 2,
        "no-irregular-whitespace": 2,
        "no-label-var": 2,
        "no-mixed-requires": [0, false],
        "linebreak-style": [0, "windows"],
        "no-multi-str": 2,
        "no-multiple-empty-lines": [1, {"max": 2}],
        "no-negated-in-lhs": 2,
        "no-nested-ternary": 0,
        "no-new": 1,
        "no-new-require": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-return-assign": 1,
        "no-self-compare": 2,
        "no-sequences": 0,
        "no-shadow": 2,
        "no-spaced-func": 2,
        "no-undef": 1,
        "no-undef-init": 2,
        "no-unexpected-multiline": 2,
        "no-underscore-dangle": 1,
        "no-unneeded-ternary": 2,
        "no-unreachable": 2,
        "no-unused-expressions": 2,
        "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
        "no-use-before-define": 2,
        "no-useless-call": 2,
        "no-var": 0,
        "array-bracket-spacing": [2, "never"],
        "arrow-parens": 0,
        "arrow-spacing": 0,
        "brace-style": [1, "1tbs"],
        "callback-return": 1,
        "camelcase": 2
    }
}